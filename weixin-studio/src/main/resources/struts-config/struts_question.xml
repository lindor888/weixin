<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
<package name="question" namespace="/question" extends="weixin">
	<action name="*Question" class="questionAction" method="{1}">
	
		<result name="jiemudan" type="dispatcher">/interact/jiemudan.jsp</result>
		<result name="saveq" type="json"><param name="root">queryJson</param></result>
		<result name="select" type="json"><param name="root">queryJson</param></result>
		<result name="list" type="json"><param name="root">queryJson</param></result>
		<result name="listoption" type="json"><param name="root">queryJson</param></result>
		<result name="option" type="json"><param name="root">queryJson</param></result>
		<result name="update" type="json"><param name="root">queryJson</param></result>
		<result name="getquestion" type="json"><param name="root">queryJson</param></result>
		
	</action>
</package>
</struts>